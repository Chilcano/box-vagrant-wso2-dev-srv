<?xml version="1.0" encoding="UTF-8"?>
<proxy name="pattern01-echo-back-proxy" startOnLoad="true" trace="disable" transports="https http" xmlns="http://ws.apache.org/ns/synapse">
  <target>
    <inSequence>
      <call-template description="Logger" target="eipt1-logger-back-template">
        <with-param name="message" value="IN_SEQ"/>
      </call-template>
      <send>
        <endpoint key="eipt1-echo-back-endpoint"/>
      </send>
    </inSequence>
    <outSequence>
      <call-template description="Logger" target="eipt1-logger-back-template">
        <with-param name="message" value="OUT_SEQ"/>
      </call-template>
      <send/>
    </outSequence>
    <faultSequence/>
  </target>
</proxy>
