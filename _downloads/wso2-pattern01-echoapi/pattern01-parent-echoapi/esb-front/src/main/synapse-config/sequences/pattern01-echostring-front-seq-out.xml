<?xml version="1.0" encoding="UTF-8"?>
<sequence name="pattern01-echostring-front-seq-out" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
         <log level="custom">
            <property name="MENSAJE" value="MENSAJE OUT DE STRING"/>
         </log>
         <payloadFactory media-type="json">
            <format>{"return_echo_cadena":$1}</format>
            <args>
               <arg evaluator="xml" expression="//node()[name()='return']"/>
            </args>
         </payloadFactory>
         <property name="messageType" value="application/json" scope="axis2"/>
         <property name="ContentType" value="application/json; charset=UTF-8" scope="axis2"/>
         <send/>
</sequence>
