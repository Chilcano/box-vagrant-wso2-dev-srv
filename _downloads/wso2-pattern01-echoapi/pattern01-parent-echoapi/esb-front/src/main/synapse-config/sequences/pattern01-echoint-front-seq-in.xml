<?xml version="1.0" encoding="UTF-8"?>
<sequence name="pattern01-echoint-front-seq-in" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
      <log level="custom">
         <property name="MENSAJE" value="MENSAJE INT DE ENTRADA"/>
         <property name="VALOR" expression="get-property('uri.var.valor')"/>
      </log>
      <payloadFactory media-type="xml">
         <format>
            <echo:echoInt xmlns:echo="http://echo.services.core.carbon.wso2.org">
               <echo:in>$1</echo:in>
            </echo:echoInt>
         </format>
         <args>
            <arg xmlns:echo="http://echo.services.core.carbon.wso2.org" evaluator="xml" expression="get-property('uri.var.valor')"/>
         </args>
      </payloadFactory>
      <property name="HTTP_METHOD" value="POST" scope="axis2"/>
      <property name="SOAPAction" value="urn:echoInt" scope="transport"/>
      <send>
         <endpoint key="pattern01-echo-front-ep"/>
      </send>
</sequence>
