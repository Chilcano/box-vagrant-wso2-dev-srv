<?xml version="1.0" encoding="UTF-8"?>
<proxy name="pattern01-echo-front-proxy" startOnLoad="true" trace="disable" transports="https http" xmlns="http://ws.apache.org/ns/synapse">
  <target>
    <inSequence>
      <header expression="synapse:get-property('MessageID')" name="SOURCE_MessageID" scope="transport"/>
      <call-template description="Logger" target="pattern01-logger-front-template">
        <with-param name="message" value="IN_SEQ"/>
      </call-template>
      <send>
        <endpoint key="pattern01-echo-front-endpoint"/>
      </send>
    </inSequence>
    <outSequence>
      <call-template description="Logger" target="pattern01-logger-front-template">
        <with-param name="message" value="OUT_SEQ"/>
      </call-template>
      <send/>
    </outSequence>
    <faultSequence/>
  </target>
</proxy>
